<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="person" class="springtest.Person">
		<constructor-arg name="id" value="12"></constructor-arg>
		<constructor-arg name="name" value="ratnesh" ></constructor-arg>
		<constructor-arg name="address" value="address" ></constructor-arg>
	</bean>

	<bean id="address" class="springtest.Address">
		<property name="lane" value="30th Cross Street"></property>
		<property name="pincode" value="63467"></property>
	</bean>
</beans>
